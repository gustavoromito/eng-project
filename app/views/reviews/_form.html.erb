<%= form_for([@event, @event.reviews.build]) do |f| %>

	<div class="field">
		<%= f.label 'Nota' %><br>
			<%= f.number_field :rate, :id => "Nota" %>
	</div>

	<div class="field">
		<%= f.label 'Comentário' %><br>
			<%= f.text_area :commentary, :id => "Comentário" %>
	</div>

	<div class="actions">
		<%= f.button 'Enviar', :value => :submit %>
	</div>
<% end %>