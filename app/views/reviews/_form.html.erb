<%= form_for([@event, @event.reviews.build]) do |f| %>

	<div class="field">
		<%= f.label 'Nota' %><br>
			<%= f.number_field 'Nota', :value => :rate %>
	</div>

	<div class="field">
		<%= f.label 'Comentário' %><br>
			<%= f.text_area 'Comentário', :value => :commentary %>
	</div>

	<div class="actions">
		<%= f.button 'Enviar', :value => :submit %>
	</div>
<% end %>